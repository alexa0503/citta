    "require": {
        "php": ">=7.2.0",
        "aliyuncs/oss-sdk-php": "~2.0"
    },
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Alex Wang",
            "email": "wanga503@outlook.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Alix\\Oss\\": "src/"
        }
    },
    "extra": {
        "laravel": {
